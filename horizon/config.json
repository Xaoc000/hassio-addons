{
  "name": "Shared CPU Agent (IBM)",
  "url": "https://github.com/dcmartin/hassio-addons",
  "version": "0.0.1",
  "slug": "hznibmcpu",
  "description": "Example Horizon addon for sharing CPU load average",
  "startup": "before",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "hassio_api": false,
  "homeassistant_api": false,
  "privileged": [
    "SYS_ADMIN"
  ],
  "timeout": 90,
  "map": [
    "share:rw",
    "config:rw"
  ],
  "options": {
    "timezone": "America/Los_Angeles",
    "horizon": {
      "device": "",
      "token": "",
      "username": "",
      "password": "",
      "exchange": "",
      "organization": ""
    },
    "kafka": {}
  },
  "schema": {
    "timezone": "str?",
    "horizon": {
      "device": "str",
      "token": "str",
      "username": "str",
      "password": "str",
      "exchange": "url",
      "organization": "str"
    },
    "kafka": {
      "user": "str",
      "password": "str",
      "instance_id": "str",
      "mqlight_lookup_url": "url",
      "api_key": "str",
      "kafka_admin_url": "url",
      "kafka_rest_url": "url",
      "kafka_brokers_sasl": [
        "str",
        "str?",
        "str?",
        "str?",
        "str?"
      ]
    }
  }
}
